{
	"name": "lyre-beacon",
	"version": "0.0.5",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./store": {
			"types": "./dist/store.d.ts",
			"svelte": "./dist/store.js"
		},
		"./rbac": {
			"types": "./dist/rbac/index.d.ts",
			"svelte": "./dist/rbac/index.js"
		},
		"./rbac/can.svelte": {
			"types": "./dist/rbac/can.svelte.d.ts",
			"svelte": "./dist/rbac/can.svelte"
		},
		"./rbac/can-any.svelte": {
			"types": "./dist/rbac/can-any.svelte.d.ts",
			"svelte": "./dist/rbac/can-any.svelte"
		},
		"./rbac/cannot.svelte": {
			"types": "./dist/rbac/cannot.svelte.d.ts",
			"svelte": "./dist/rbac/cannot.svelte"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"svelte": "./dist/server/index.js"
		},
		"./hooks.server": {
			"types": "./dist/hooks.server.d.ts",
			"svelte": "./dist/hooks.server.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0",
		"@sveltejs/kit": "^2.22.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"keywords": [
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"set-cookie-parser": "^2.7.1",
		"svelte-kit-cookie-session": "^4.1.1"
	},
	"license": "MIT"
}
